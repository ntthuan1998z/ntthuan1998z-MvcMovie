@model IEnumerable<Movie>

<table>
    <thead>
        <th>Title</th>
        <th>Genre</th>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
            <tr>
                <td>@movie.Title</td>
                <td>@movie.Genre</td>
            </tr>            
        }
    </tbody>
</table>